/boot		emmc      /dev/block/mmcblk0p7
/cache		ext4      /dev/block/mmcblk0p14
/data		ext4      /dev/block/mmcblk0p22 length=-16384
/misc		emmc      /dev/block/mmcblk0p16
/recovery	emmc      /dev/block/mmcblk0p15
/external_sd	vfat      /dev/block/mmcblk1p1        /dev/block/mmcblk1	flags=display="Micro SDcard";storage;wipeingui;removable
/system		ext4      /dev/block/mmcblk0p12
/sdcard		vfat      /dev/block/mmcblk0p21					flags=display="internal SD";storage;wipeingui;
/splash		emmc      /dev/block/mmcblk0p20					flags=display=Splash;backup=1
